{
    "dataset": "newfacade/LeetCodeDataset",
    "model_name": "Qwen/Qwen2.5-7B-Instruct",
    "ckpt_name": "qwen_checkpoint",
    "sft": {
        "num_epochs": 3,
        "batch_size": 16,
        "lr": 2e-5,
        "warmup": 0.05,
        "grad_accum_steps": 4
    },
    "lora": {
        "rank": 8,
        "alpha": 32,
        "target": ["q_proj", "v_proj", "o_proj"],
        "dropout": 0.05
    },
    "generation": {
        "max_new_tokens": 2048,
        "temperature": 0.5,
        "top_p": 0.95
    }
}